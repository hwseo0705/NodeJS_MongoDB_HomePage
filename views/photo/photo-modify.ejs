<html lang="ko">

<head>
    <%- include('../partials/head.ejs')%>

    <title>AVIP Lab - Photos</title>

    <style>
        @import url("/css/footer.css");
        @import url("/css/header.css");
        @import url("/css/form-common.css");
        
        .wrap .content form textarea {
            font-size: 17px;
            padding: 5px;
        }
    </style>
</head>

<body>

    <%- include('../partials/header.ejs')%>
    <div class="wrap">
        <div class="box">
            <h3>Modify Photos</h3>
        </div>

        <div class="content">
            <form action="/photo/modify?id=<%= result._id %>&_method=PUT" method="POST">
                <label for="img">Image</label>
                <input type="file" id="img" name="img" placeholder="img" required>
                <button>Submit</button>
            </form>
        </div>
    </div>
    <%- include('../partials/footer.ejs')%>

</body>

</html>
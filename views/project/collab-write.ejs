<html lang="ko">

<head>
    <%- include('../partials/head.ejs')%>

    <title>AVIP Lab - Projects</title>

    <style>
        @import url("/css/footer.css");
        @import url("/css/header.css");
        @import url("/css/form-common.css");
    </style>
</head>

<body>

    <%- include('../partials/header.ejs')%>
    <div class="wrap">
        <div class="box">
            <h3>Add Collaborators</h3>
        </div>

        <div class="content">
            <form action="/project/collab/write" method="POST">
                <label for="institution">Institution</label>
                <input type="text" id="institution" name="institution" placeholder="institution" required>       
                <label for="name">Name</label>
                <input type="text" id="name" name="name" placeholder="name" required>
                <label for="img">Image</label>
                <input type="file" id="img" name="img" placeholder="img" required>       
                <button>Submit</button>
            </form>
        </div>
    </div>
    <%- include('../partials/footer.ejs')%>

</body>

</html>